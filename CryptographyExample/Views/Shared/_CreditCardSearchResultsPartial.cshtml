@model IEnumerable<CryptographyExample.Models.CreditCardModels.CreditCardViewModel>

@{
    ViewData["Title"] = "Results";
}

<h2>Results</h2>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreationTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Company)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayTextFor(model => item.CreationTime)
            </td>
            <td>
                @Html.DisplayTextFor(model => item.Company)
            </td>
            <td>@Html.ActionLink("Details", "Details", "CreditCard", routeValues: new { id = item.Id }, htmlAttributes: new { @class = "btn btn-info" })</td>
        </tr>
    }
    </tbody>
</table>
